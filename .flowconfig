[ignore]
dist/.*
build/.*
examples/.*
.*/node_modules/object.*
.*/node_modules/eslint.*
.*/node_modules/glob.*
.*/node_modules/bab.*
.*/node_modules/es.*
.*/node_modules/lodash.*
.*/node_modules/array.*
.*/node_modules/is-.*
.*/node_modules/object.*
.*/node_modules/acorn.*
.*/node_modules/fb.*
.*/node_modules/js.*
.*/node_modules/b.*
.*/node_modules/spdx.*

[libs]
node_modules/reactive-di/lib

[options]
munge_underscores=true
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
suppress_comment= \\(.\\|\n\\)*\\$FlowDisable
module.name_mapper='.*\(\.css\)' -> 'empty/object'
module.name_mapper='.*\(\.configloaderrc\)' -> 'empty/object'
module.name_mapper='^regexp-input-demo/i/\(.*\)' -> '<PROJECT_ROOT>/i/\1'
module.name_mapper='^regexp-input-demo/\(.*\)' -> '<PROJECT_ROOT>/src/\1'
